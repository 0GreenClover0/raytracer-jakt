import vec3

fn write_color(pixel_color: &vec3) {
    let r = pixel_color.x
    let g = pixel_color.y
    let b = pixel_color.z

    let rbyte = (255.999f32 * r) as! i32
    let gbyte = (255.999f32 * g) as! i32
    let bbyte = (255.999f32 * b) as! i32

    print("{} {} {}\n", rbyte, gbyte, bbyte)
}
